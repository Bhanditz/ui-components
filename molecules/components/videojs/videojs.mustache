{{# videojs }}
<video id="videojs-viewer" class="video-js vjs-default-skin" width="640" height="264" poster="{{ poster }}" controls>
  {{# sources }}
  <source src="{{ src }}" type="{{ mime_type }}">
  {{/ sources }}
  <p class="no-js">{{ i18n.global.requires_javascript }}</p>
</video>
{{/ videojs }}