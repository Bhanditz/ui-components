{{# is_audio }}
<audio id="videojs-viewer" class="audio-js vjs-default-skin" width="640" height="264" poster="{{ poster }}" controls {{# tech_order }}data-tech-order="{{ tech_order }}"{{/ tech_order }}>
  {{# sources }}
  <source {{# src}}src="{{ src }}"{{/ src}} {{# mime_type }}type="{{ mime_type }}"{{/ mime_type}}>
  {{/ sources }}
  <p class="no-js">{{ i18n.global.requires_javascript }}</p>
</audio>
{{/ is_audio }}
{{# is_video }}
<video id="videojs-viewer" class="video-js vjs-default-skin" width="640" height="264" poster="{{ poster }}" controls {{# tech_order }}data-tech-order="{{ tech_order }}"{{/ tech_order }}>
  {{# sources }}
  <source {{# src}}src="{{ src }}"{{/ src}} {{# mime_type }}type="{{ mime_type }}"{{/ mime_type}}>
  {{/ sources }}
  <p class="no-js">{{ i18n.global.requires_javascript }}</p>
</video>
{{/ is_video }}