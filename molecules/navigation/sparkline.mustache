<div class="sparkline">
    <div class="sparkline-data">
        {{#data}}
        <a class="index" href="{{url}}"><span class="count" style="height: {{percent_of_max}}%;">{{value}}</span> </a>
        {{/data}}
    </div>

    <div class="sparkline-range">
      <span class="sparkline-start">{{date_start}}</span>
      <span class="sparkline-end">{{date_end}}</span>
    </div>
</div>